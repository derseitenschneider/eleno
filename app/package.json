{
  "name": "eleno",
  "private": true,
  "version": "2.4.3",
  "type": "module",
  "homepage": "https://app.eleno.net",
  "main": "./index.html",
  "scripts": {
    "dev:staging": "vite --mode staging --open",
    "dev:prod": "vite --mode production --open --host",
    "build": "vite build",
    "build:staging": "vite build --mode staging ",
    "build:ci": "vite build && vite preview --host --port 5000",
    "preview": "vite preview",
    "generate-types:prod": "npx supabase gen types typescript --project-id 'brhpqxeowknyhrimssxw' --schema public > src/types/supabase.ts",
    "generate-types:staging": "npx supabase gen types typescript --project-id 'nrzxjxtylpazkoieqjvt' --schema public > src/types/supabase.ts",
    "generate-types:local": "npx supabase gen types typescript --local > src/types/supabase.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "test:cov:check": "npm run test:cov && node scripts/check-coverage-thresholds.js",
    "test:cov:badges": "npm run test:cov && node scripts/generate-coverage-badges.js",
    "coverage:check": "node scripts/check-coverage-thresholds.js",
    "coverage:badges": "node scripts/generate-coverage-badges.js",
    "coverage:bypass-report": "node scripts/generate-bypass-report.js",
    "coverage:bypass-report:monthly": "node scripts/generate-bypass-report.js --month=$(date +%Y-%m)",
    "coverage:bypass-report:json": "node scripts/generate-bypass-report.js --format=json --output=coverage-bypass-report.json",
    "test:perf": "time vitest run --reporter=verbose",
    "test:benchmark": "node scripts/test-benchmark.js",
    "test:single": "vitest run --pool=forks --poolOptions.forks.singleFork=true",
    "test:parallel": "vitest run --pool=threads --poolOptions.threads.maxThreads=6",
    "test:flaky-detect": "node scripts/detect-flaky-tests.js",
    "test:flaky-detect-quick": "node scripts/detect-flaky-tests.js 5 2",
    "test:flaky-detect-thorough": "node scripts/detect-flaky-tests.js 20 4",
    "typecheck": "tsc --noEmit 2>&1 | grep -v 'node_modules/' || true",
    "pw": "NODE_NO_WARNINGS=1 playwright test",
    "pw:subs": "npm run pw -- --project='*subscription*'",
    "pw:codegen": "playwright codegen --load-storage=playwright/.auth/user.json http://localhost:5173",
    "pw:local": "LOCAL_TEST=1 NODE_NO_WARNINGS=1 playwright test",
    "pw:debug": "npm run pw -- --debug",
    "pw:local:debug": "npm run pw:local -- --debug",
    "pw:ui": "npm run pw -- --ui",
    "pw:local:ui": "npm run pw:local -- --ui",
    "pw:headed": "npm run pw -- --headed",
    "pw:local:headed": "npm run pw:local -- --headed",
    "pw:html": "PLAYWRIGHT_HTML_OPEN=always npm run pw -- --reporter=html",
    "pw:repeat": "npm run pw -- --repeat-each 5",
    "pw:repeat:med": "npm run pw -- --repeat-each 15",
    "pw:repeat:max": "npm run pw -- --repeat-each 50",
    "pw:local:repeat": "npm run pw:local -- --repeat-each 5",
    "monitoring:collect": "node scripts/test-metrics-collector.js collect",
    "monitoring:trends": "node scripts/test-metrics-collector.js trends",
    "monitoring:alerts": "node scripts/test-metrics-collector.js alerts",
    "monitoring:summary": "node scripts/test-metrics-collector.js summary",
    "monitoring:dashboard": "node scripts/test-dashboard-generator.js generate",
    "monitoring:dashboard:open": "node scripts/test-dashboard-generator.js generate && open .test-metrics/dashboard/index.html",
    "monitoring:performance": "node scripts/performance-monitor.js benchmark",
    "monitoring:regressions": "node scripts/performance-monitor.js regressions",
    "monitoring:recommendations": "node scripts/performance-monitor.js recommendations",
    "monitoring:notify": "node scripts/test-alerting-system.js notify",
    "monitoring:report:week": "node scripts/test-reporting-automation.js generate week",
    "monitoring:report:month": "node scripts/test-reporting-automation.js generate month",
    "monitoring:quality": "node scripts/test-reporting-automation.js quality week",
    "pw:local:repeat:med": "npm run pw:local -- --repeat-each 15",
    "pw:local:repeat:max": "npm run pw:local -- --repeat-each 50",
    "pw:accessibility": "npm run pw -- --project='*accessibility*'",
    "pw:accessibility:local": "npm run pw:local -- --project='*accessibility*'",
    "pw:accessibility:headed": "npm run pw -- --project='*accessibility*' --headed",
    "pw:accessibility:debug": "npm run pw -- --project='*accessibility*' --debug",
    "pw:performance": "npm run pw -- --project='*performance*'",
    "pw:performance:local": "npm run pw:local -- --project='*performance*'",
    "pw:performance:headed": "npm run pw -- --project='*performance*' --headed",
    "pw:performance:debug": "npm run pw -- --project='*performance*' --debug",
    "pw:edge-case": "npm run pw -- --project='*edge-case*'",
    "pw:edge-case:local": "npm run pw:local -- --project='*edge-case*'",
    "pw:edge-case:headed": "npm run pw -- --project='*edge-case*' --headed",
    "pw:edge-case:debug": "npm run pw -- --project='*edge-case*' --debug",
    "pw:cross-browser": "npm run pw -- --project='*cross-browser*'",
    "pw:cross-browser:local": "npm run pw:local -- --project='*cross-browser*'",
    "pw:cross-browser:critical": "npm run pw -- --project='*cross-browser-critical*'",
    "pw:cross-browser:subscription": "npm run pw -- --project='*cross-browser-subscription*'",
    "pw:cross-browser:responsive": "npm run pw -- --project='*cross-browser-responsive*'",
    "pw:cross-browser:compatibility": "npm run pw -- --project='*cross-browser-compatibility*'",
    "pw:cross-browser:chromium": "npm run pw -- --project='*chromium*'",
    "pw:cross-browser:firefox": "npm run pw -- --project='*firefox*'",
    "pw:cross-browser:webkit": "npm run pw -- --project='*webkit*'",
    "pw:cross-browser:headed": "npm run pw -- --project='*cross-browser*' --headed",
    "pw:cross-browser:debug": "npm run pw -- --project='*cross-browser*' --debug",
    "styleguide": "vite serve styleguide --port 5174 --open",
    "styleguide:build": "echo 'Styleguide is static HTML, no build needed'",
    "pw:edge-case:update": "npm run pw -- --project='*edge-case*' --update-snapshots",
    "analyze-bundle": "node scripts/analyze-bundle.js",
    "test:report": "node scripts/generate-test-summary.js github",
    "test:coverage-summary": "node scripts/generate-test-summary.js coverage",
    "test:playwright-summary": "node scripts/generate-test-summary.js playwright",
    "test:badges": "node scripts/generate-test-summary.js badges",
    "test:open-coverage": "open coverage/index.html || start coverage/index.html || xdg-open coverage/index.html",
    "test:open-playwright": "npx playwright show-report",
    "test:cross-browser-report": "node scripts/generate-cross-browser-report.js",
    "test:cross-browser-report:json": "node scripts/generate-cross-browser-report.js --format=json --output=cross-browser-report.json",
    "test:critical": "npm run pw -- --project='*cross-browser-critical*' --project='*edge-case*' tests/share-homework/share-homework.spec.ts",
    "test:critical:local": "npm run pw:local -- --project='*cross-browser-critical*' --project='*edge-case*' tests/share-homework/share-homework.spec.ts",
    "test:before-deploy": "npm run typecheck && npm run test && npx biome check --max-diagnostics=50",
    "test:smoke": "node scripts/smoke-test.js"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.6.0",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.3.6",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@react-pdf/renderer": "^3.1.14",
    "@stripe/react-stripe-js": "^2.8.0",
    "@stripe/stripe-js": "^4.6.0",
    "@supabase/gotrue-js": "2.64.4",
    "@supabase/supabase-js": "^2.50.0",
    "@tanstack/react-form": "^1.12.1",
    "@tanstack/react-query": "^5.40.0",
    "@tanstack/react-table": "^8.17.3",
    "@tanstack/zod-form-adapter": "^0.42.1",
    "cmdk": "^1.0.0",
    "date-fns": "^2.30.0",
    "export-from-json": "^1.7.3",
    "framer-motion": "^10.16.4",
    "html-react-parser": "^4.0.0",
    "jszip": "^3.10.1",
    "localforage": "^1.10.0",
    "lucide-react": "^0.344.0",
    "match-sorter": "^6.3.1",
    "next-themes": "^0.3.0",
    "react-csv": "^2.2.2",
    "react-csv-downloader": "^3.0.0",
    "react-day-picker": "^8.10.1",
    "react-hook-form": "^7.51.1",
    "react-icons": "^4.7.1",
    "react-pdf-html": "^1.1.18",
    "react-resizable-panels": "^2.1.7",
    "react-router-dom": "^6.8.0",
    "react-simple-wysiwyg": "^2.2.5",
    "react-spinners": "^0.13.8",
    "sonner": "^1.4.41",
    "sort-by": "^0.0.2",
    "stripe": "^17.7.0",
    "vaul": "^1.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@biomejs/biome": "2.0.6",
    "@playwright/test": "^1.51.0",
    "@tanstack/react-query-devtools": "^5.49.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^18.16.13",
    "@types/react": "^18.0.27",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.7",
    "jsdom": "^24.0.0",
    "msw": "^2.10.4",
    "postcss": "^8.4.35",
    "sass": "^1.62.1",
    "supabase": "^1.142.2",
    "tailwind-merge": "^2.3.0",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3",
    "vite": "^6.3.5",
    "vite-plugin-preload": "^0.3.1",
    "vite-plugin-pwa": "^1.0.0",
    "vitest": "^3.0.8"
  },
  "resolutions": {
    "@radix-ui/react-dismissable-layer": "^1.1.5"
  }
}
